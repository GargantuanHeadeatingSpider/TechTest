@model UserManagement.Models.LogViewModel

<h2>Log Details</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">Title</dt>
        <dd class="col-sm-10">@Model.Title</dd>
        <dt class="col-sm-2">Type</dt>
        <dd class="col-sm-10">@Model.LogType</dd>
        <dt class="col-sm-2">Description</dt>
        <dd class="col-sm-10">@Model.Description</dd>
        <dt class="col-sm-2">Date Created</dt>
        <dd class="col-sm-10">@Model.DateCreated.ToString("dd/MM/yyyy")</dd>
    </dl>
    <a asp-action="Index" class="btn btn-primary">Back to List</a>
</div>
